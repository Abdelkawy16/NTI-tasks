{{>header}}
{{>navbar}}
<section class="container my-5">
    {{#unless task}}
        <div class="alert alert-danger">task not found</div>
    {{else}}
        <div class="border border-danger border-2 p-3">
            <h3>task Id: {{task._id}}</h3>
            <h3>task Name: {{task.taskType}}</h3>
            <h3>task Name: {{task.taskName}}</h3>
        </div>
    {{/unless}}

</section>
{{>footer}}